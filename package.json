{
	"name": "trail",
	"version": "4.8.0",
	"description": "Add typed relations to your notes and traverse them via a powerful hierarchy view.",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node scripts/build-grammar.mjs && node esbuild.config.mjs",
		"build": "node scripts/build-grammar.mjs && tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"build:grammar": "node scripts/build-grammar.mjs",
		"bump": "node version-bump.mjs",
		"lint": "eslint .",
		"lint:docs": "node scripts/validate-doc-links.mjs",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"keywords": [],
	"license": "0-BSD",
	"devDependencies": {
		"@codemirror/autocomplete": "^6.20.0",
		"@codemirror/commands": "^6.10.1",
		"@codemirror/language": "^6.12.1",
		"@codemirror/lint": "^6.9.2",
		"@codemirror/state": "^6.5.0",
		"@codemirror/view": "^6.39.11",
		"@eslint/js": "9.30.1",
		"@eslint/json": "^0.14.0",
		"@lezer/generator": "^1.8.0",
		"@lezer/highlight": "^1.2.3",
		"@lezer/lr": "^1.4.7",
		"@types/node": "^16.11.6",
		"esbuild": "0.25.5",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "14.0.0",
		"jiti": "2.6.1",
		"tslib": "2.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.1",
		"vitest": "^4.0.17"
	},
	"dependencies": {
		"obsidian": "latest"
	}
}